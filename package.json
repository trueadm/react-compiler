{
  "name": "react-compiler",
  "version": "1.0.0",
  "description": "React compiler project",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir lib --source-maps",
    "watch": "babel src scripts --out-dir lib --watch --source-maps --verbose",
    "test-ssr": "babel-node --expose-gc scripts/test.js --mode=ssr",
    "test-dom": "babel-node --expose-gc scripts/test.js --mode=dom",
    "test": "yarn test-ssr",
    "sandbox": "node lib/cli.js --entry=node-sandbox/App.js"
  },
  "author": "Dominic Gannaway",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.1.5",
    "@babel/generator": "^7.1.6",
    "@babel/parser": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/traverse": "^7.1.6",
    "@babel/types": "^7.1.5",
    "chalk": "^2.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/plugin-transform-react-jsx": "^7.1.6",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.9.0",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-flow-header": "^0.2.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-header": "^2.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "glob": "^7.1.3",
    "google-closure-compiler": "^20190121.0.0",
    "jsdom": "^13.0.0",
    "prettier": "^1.15.2",
    "react": "^16.8.0",
    "react-compiler-runtime": "file:scripts/compiler-runtime",
    "react-dom": "^16.8.0",
    "simple-html-dom-parser": "^1.1.2",
    "tmp": "^0.0.33"
  }
}
