{
  "version": "0.0.1",
  "description": "Browser sandbox for the React compiler",
  "scripts": {
    "compile": "node ../lib/cli.js --entry=src/App.js",
    "build-compiled-bundle": "rollup --config compiled-rollup-config.js",
    "build-react-bundle": "rollup --config react-rollup-config.js",
    "build-inferno-bundle": "rollup --config inferno-rollup-config.js",
    "build-svelte-bundle": "rollup --config svelte-rollup-config.js",
    "build": "yarn compile && yarn build-compiled-bundle && yarn build-react-bundle && yarn build-inferno-bundle && yarn build-svelte-bundle"
  },
  "author": "Dominic Gannaway",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-syntax-flow": "^7.2.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-flow-strip-types": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "babel-plugin-inferno": "^6.0.4",
    "cx": "file:../scripts/cx",
    "fbt": "file:../scripts/fbt",
    "inferno": "^7.0.5",
    "react": "16.8.0-alpha.0",
    "react-compiler-runtime": "file:../scripts/compiler-runtime/",
    "react-dom": "16.8.0-alpha.0",
    "rollup": "^1.0.1",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-google-closure-compiler": "^0.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-svelte": "^5.0.1",
    "svelte": "^2.16.0"
  }
}
